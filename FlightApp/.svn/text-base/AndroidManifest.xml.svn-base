<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="b07.flightapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/vector_air"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CentralActivity"
            android:label="@string/title_activity_central" >
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name=".PersonalInfoActivity"
            android:label="@string/title_activity_personal_info" >
        </activity>
        <activity
            android:name=".EditPersonalActivity"
            android:label="@string/title_activity_edit_personal" >
        </activity>
        <activity
            android:name=".BookedActivity"
            android:label="@string/title_activity_booked" >
        </activity>
        <activity
            android:name=".BookActivity"
            android:label="@string/title_activity_book" >
        </activity>
        <activity
            android:name=".UserBookActivity"
            android:label="@string/title_activity_user_book" >
        </activity>
        <activity
            android:name=".ViewClients"
            android:label="@string/title_activity_view_clients" >
        </activity>
        <activity
            android:name=".EditFlight"
            android:label="@string/title_activity_edit_flight" >
        </activity>
        <activity
            android:name=".UploadActivity"
            android:label="@string/title_activity_upload" >
        </activity>
    </application>

</manifest>
